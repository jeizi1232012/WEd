<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.lance.freebook.MVP.Home.Fragment.StackInfoFragment">
    <com.xiaochao.lcrapiddeveloplibrary.viewtype.ProgressActivity
        xmlns:progressActivity="http://schemas.android.com/apk/res-auto"
        android:id="@+id/stack_info_progress"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            >
            <com.xiaochao.lcrapiddeveloplibrary.widget.SpringView
                android:id="@+id/stack_info_springview"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                >
                <android.support.v7.widget.RecyclerView
                    android:id="@+id/stack_info_list"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="@color/window_gray"/>
            </com.xiaochao.lcrapiddeveloplibrary.widget.SpringView>
        </LinearLayout>
    </com.xiaochao.lcrapiddeveloplibrary.viewtype.ProgressActivity>
</LinearLayout>
